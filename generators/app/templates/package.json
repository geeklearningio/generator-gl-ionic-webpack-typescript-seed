  "bugs": {
    "url": "https://github.com/geeklearningio/gl-ionic-webpack-seed/issues"
  },
  "homepage": "https://github.com/geeklearningio/gl-ionic-webpack-seed#readme",
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "awesome-typescript-loader": "^3.1.3",
    "browser-sync": "^2.18.8",
    "browser-sync-webpack-plugin": "^1.1.4",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-util": "^3.0.8",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "json-loader": "^0.5.4",
    "ng-annotate-webpack-plugin": "^0.2.1-pre",
    "ng-cordova": "^0.1.27-alpha",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.2",
    "tslint": "^5.2.0",
    "tslint-loader": "^3.5.3",
    "webpack": "^1.15.0"
  },
  "dependencies": {
    "angular-i18n": "^1.6.4",
    "angular-moment": "^1.0.1",
    "gl-angular-configuration": "git+https://github.com/geeklearningio/gl-angular-configuration.git",
